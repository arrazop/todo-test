{
  "name": "todo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "e2e:test:debug": "detox test -c ios.sim.debug",
    "e2e:build:debug": "detox build -c ios.sim.debug",
    "e2e:test": "detox test -c ios.sim.release",
    "e2e:build": "detox build -c ios.sim.release"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@reduxjs/toolkit": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-flipper": "^0.212.0",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-svg": "^14.1.0",
    "react-redux": "^9.1.0",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.4",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-styled-components": "^2.1.4",
    "detox": "^20.18.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.release": {
        "binaryPath": "./ios/build/Build/Products/Release-iphonesimulator/todo.app",
        "build": "xcodebuild -workspace ios/todo.xcworkspace -configuration release -scheme todo -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 15 Pro"
      }
    }
  }
}
